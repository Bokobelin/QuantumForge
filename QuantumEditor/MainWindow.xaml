<Window x:Class="QuantumEditor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QuantumEditor"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.DataContext>
        <Binding Source="QuantumEditor/MainWindow" />
    </Window.DataContext>
    <Grid>
        <ListBox x:Name="loggerListBox" Width="600" Height="100" VerticalAlignment="Bottom" HorizontalAlignment="Left" ItemsSource="{Binding logs}">

        </ListBox>
    </Grid>
</Window>
